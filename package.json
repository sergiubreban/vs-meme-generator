{
	"name": "memozaur",
	"displayName": "memozaur",
	"description": "meme recreation tool",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:memozaur.memeGenerator",
		"onView:meme-sidebar"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "meme-sidebar-view",
					"title": "Meme Sidebar View",
					"icon": "media/icon.svg"
				}
			]
		},
		"views": {
			"meme-sidebar-view": [
				{
					"type": "webview",
					"id": "meme-sidebar",
					"name": "Meme Sidebar",
					"icon": "media/icon.svg",
					"contextualTitle": "brb"
				}
			]
		},
		"commands": [
			{
				"command": "memozaur.memeGenerator",
				"title": "Meme generator"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "rollup -c && tsc -p ./",
		"watch": "concurrently \"rollup -c -w\" \"tsc -watch -p ./\"",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.38.2",
		"svelte-check": "^1.5.4",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"@rollup/plugin-typescript": "^8.2.1",
		"concurrently": "^6.1.0",
		"meme-api": "^1.0.0",
		"svelte-preprocess": "^4.7.3"
	}
}
